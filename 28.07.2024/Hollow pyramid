def hollow_pyramid(n):
    for i in range(n):
        print(' ' * (n - i - 1), end='')
        if i == 0:
            print('*')  
        elif i == n - 1:
            print('*' * (2 * i + 1))  
        else:
            print('*' + ' ' * (2 * i - 1) + '*')  

rows = int(input())
hollow_pyramid(rows)
